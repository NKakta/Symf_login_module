{% extends 'layouts/base.html.twig' %}

{% block title %} CV paraiškos {% endblock %}

{% block body %}
    <a type="button" class="btn btn-sm btn-outline-secondary" href="{{ path('resume_index') }}">Grįžti į CV sąrašą</a>
    {{ form_start(form)}}
    {{ form_row(form.area) }}
    {{ form_row(form.education) }}
    {{ form_row(form.languages) }}
    {{ form_row(form.salary) }}
    {{ form_row(form.experience) }}
    {{ form_row(form.aboutYou) }}
    {{ form_row(form.usersToHide) }}
    {{ form_row(form.isMain) }}
    <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
    <button type="submit">Patvirtinti</button>
    {{ form_end(form) }}
{% endblock %}
